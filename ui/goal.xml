<rdf:RDF xmlns="http://www.semanticweb.org/robot-pathfinding#"
     xml:base="http://www.semanticweb.org/robot-pathfinding"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Онтология для поиска пути роботом -->
    <owl:Ontology rdf:about="http://www.semanticweb.org/robot-pathfinding"/>

    <!-- Классы -->
    <owl:Class rdf:about="#Goal"/>

    <!-- Свойства -->
    <owl:ObjectProperty rdf:about="#has_position"/>
    <owl:DatatypeProperty rdf:about="#x_position">
        <rdfs:domain rdf:resource="#Goal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#y_position">
        <rdfs:domain rdf:resource="#Goal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <!-- Индивидуумы для препятствий -->
    <owl:NamedIndividual rdf:about="#Goal_1">
        <rdf:type rdf:resource="#Goal"/>
        <x_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</x_position>
        <y_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</y_position>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Goal_2">
        <rdf:type rdf:resource="#Goal"/>
        <x_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</x_position>
        <y_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</y_position>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Goal_3">
        <rdf:type rdf:resource="#Goal"/>
        <x_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</x_position>
        <y_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</y_position>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Goal_4">
        <rdf:type rdf:resource="#Goal"/>
        <x_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</x_position>
        <y_position rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</y_position>
    </owl:NamedIndividual>

</rdf:RDF>
